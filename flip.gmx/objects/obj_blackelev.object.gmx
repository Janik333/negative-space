        if (y%64 == 0)
            instance_create (x,y,obj_blackwall);
    }
}else{
    if (y &lt; ystart)
    {
        y+=4;
        //move black player down with elevator
        if (place_meeting(x,y-5,obj_blackdude))
            obj_blackdude.vsp += 4;
        //destroy elevator shaft on way down
        with(instance_position(x,y-4,obj_blackwall))
            instance_destroy();
        //so white player doesnt get stuck
        if (collision_point(x+31,y,obj_whitedude,false,false) || collision_point(x+32,y,obj_whitedude,false,false))
            obj_whitedude.y += 4;
    }
}

//creates white wall above elevator
if (yprevious != y)
{
    for (var i = 0; i &lt; 64 ; i+=4)
    {
        j = y-i;
        if (!collision_point(x,j,obj_blackwall,false,false))
            if ( j &gt; ystart - 132)
                instance_create (x,j,obj_whitepixelvert);
    }
}


//destroy white walls in elevator (so white player can go through)
var j;
for (j = 0; j &lt; 64; j++)
        with(instance_position(x,y+j,obj_whitewall))
